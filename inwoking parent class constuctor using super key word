import java.io.*;
import java.util.*;
class Animal{
    Animal(){
        System.out.println("Animal is created");
    }
}
class Dog extends Animal{
    Dog(){              // sub class constuctor
        super();       // inwokes parent class constructor
        System.out.println("Dog is created");
    }
}
class SuperDemo1{
    public static void main(String args[]){
        Dog d=new Dog();
    }
}
